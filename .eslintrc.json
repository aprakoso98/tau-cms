{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0,
        "react/display-name": 0,
        "react/no-children-prop": 0,
        "react/no-mixed-operators": 0,
        "react/no-useless-escape": 0,
        "react/no-extend-native": 0,
        "jsx-a11y/anchor-is-valid": 0
    }
}